{
  "compilerOptions": {
    "jsx": "react-jsx",
    "baseUrl": ".",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "module": "ES2020",
    "moduleResolution": "node",
    "target": "ES2020",
    "types": ["vite/client"]
  },
  "include": ["src"]
}